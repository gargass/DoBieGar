czestosci = matrix(nrow = 10*14, ncol=297+2)
for (i in 1:length(nowotwory)){
dane <- get(paste(nowotwory[i], '_variant', sep=""))
n = nrow(dane)/10
for (j in 1:length(varianty)){
czestosci[(i - 1)*length(varianty) + j, 1] <- nowotwory[i]
